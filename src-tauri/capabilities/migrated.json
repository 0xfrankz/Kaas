{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPDATA/*",
        "$APPDATA/cache/*",
        "$RESOURCE/*",
        "$RESOURCE/resources/*"
      ]
    },
    "shell:allow-open",
    "dialog:allow-save",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:default",
    "shell:default"
  ]
}